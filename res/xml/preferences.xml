<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <Preference android:title="@string/pref_set_wallpaper" android:key="set_wallpaper"/>

    <PreferenceCategory
    android:title="@string/pref_flock_category_title"
    android:summary="@string/pref_flock_category_summary">

   <CheckBoxPreference
      android:key="SCALE_FLOCK"
      android:title="@string/pref_scale_flock_title"
      android:summary="@string/pref_scale_flock_summary" />  

   <org.quuux.boids.SeekBarPreference
      android:key="FLOCK_SIZE"
      android:title="@string/pref_flock_size_title"
      android:summary="@string/pref_flock_size_summary"
      android:dialogMessage="@string/pref_flock_size_dialog_message"
      android:defaultValue="500"
      android:text="@string/percent"
      android:max="1000" />

   <org.quuux.boids.SeekBarPreference
       android:key="BONUS_VELOCITY"
       android:title="@string/pref_velocity_title"
       android:summary="@string/pref_velocity_summary"
       android:dialogMessage="@string/pref_velocity_dialog_message"
       android:defaultValue="100"
       android:text="@string/percent"
       android:max="200" />

   <org.quuux.boids.SeekBarPreference
      android:key="RANGE"
      android:title="@string/pref_range_title"
      android:summary="@string/pref_range_summary"
      android:dialogMessage="@string/pref_range_dialog_message"
      android:defaultValue="100"
      android:text="@string/percent"
      android:max="1000" />

   <org.quuux.boids.SeekBarPreference
      android:key="NEIGHBORS"
      android:title="@string/pref_neighbors_title"
      android:summary="@string/pref_neighbors_summary"
      android:dialogMessage="@string/pref_neighbors_dialog_message"
      android:defaultValue="10"
      android:text="@string/percent"
      android:max="1000" />

   <org.quuux.boids.SeekBarPreference
      android:key="BONUS_V1"
      android:title="@string/pref_v1_title"
      android:summary="@string/pref_v1_summary"
      android:dialogMessage="@string/pref_v1_dialog_message"
      android:defaultValue="100"
      android:text="@string/percent"
      android:max="200" />

   <org.quuux.boids.SeekBarPreference
      android:key="BONUS_V2"
      android:title="@string/pref_v2_title"
      android:summary="@string/pref_v2_summary"
      android:dialogMessage="@string/pref_v2_dialog_message"
      android:defaultValue="100"
      android:text="@string/percent"
      android:max="200" />

   <org.quuux.boids.SeekBarPreference
      android:key="BONUS_V3"
      android:title="@string/pref_v3_title"
      android:summary="@string/pref_v3_summary"
      android:dialogMessage="@string/pref_v3_dialog_message"
      android:defaultValue="100"
      android:text="@string/percent"
      android:max="200" />

   <org.quuux.boids.SeekBarPreference
      android:key="BONUS_V4"
      android:title="@string/pref_v4_title"
      android:summary="@string/pref_v4_summary"
      android:dialogMessage="@string/pref_v4_dialog_message"
      android:defaultValue="100"
      android:text="@string/percent"
      android:max="200" />

   <org.quuux.boids.SeekBarPreference
      android:key="BONUS_V5"
      android:title="@string/pref_v5_title"
      android:summary="@string/pref_v5_summary"
      android:dialogMessage="@string/pref_v5_dialog_message"
      android:defaultValue="100"
      android:text="@string/percent"
      android:max="200" />

   <org.quuux.boids.SeekBarPreference
      android:key="BONUS_V6"
      android:title="@string/pref_v6_title"
      android:summary="@string/pref_v6_summary"
      android:dialogMessage="@string/pref_v6_dialog_message"
      android:defaultValue="100"
      android:text="@string/percent"
      android:max="200" />

   <org.quuux.boids.SeekBarPreference
      android:key="FLEE_TIME"
      android:title="@string/pref_flee_time_title"
      android:summary="@string/pref_flee_time_summary"
      android:dialogMessage="@string/pref_flee_time_dialog_message"
      android:defaultValue="3000"
      android:text="@string/percent"
      android:max="10000" />

 </PreferenceCategory>

</PreferenceScreen>

